@model LearnerProject.Models.Entities.Contact
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<br />
<h3>Update New Contact</h3>
<br />
<form method="post">
    @Html.HiddenFor(x => x.ContactId)
    <label>Address</label>
    @Html.TextBoxFor(x => x.Address, new { @class = "form-control" })
    <br />
    <label>Open Hours</label>
    @Html.TextBoxFor(x => x.OpenHours, new { @class = "form-control" })
    <br />
    <label>Email</label>
    @Html.TextAreaFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Phone</label>
    @Html.TextAreaFor(x => x.Phone, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary">Update</button>
</form>